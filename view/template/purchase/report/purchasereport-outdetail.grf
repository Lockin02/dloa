			QuerySQL='select ig.productCode,ig.productName,ig.actNum,\r\n		psub.orderCode,psub.number,psub.executedNum,psub.notExeNum,psub.deliveryDate from (\r\nselect if(o.orderCode='',o.orderTempCode,o.orderCode) as orderCode,o.deliveryDate,\r\n  			oe.`productId` ,oe.`number`,oe.`executedNum`,\r\n            (oe.`number`-oe.`executedNum`) as notExeNum \r\n  			FROM oa_sale_order_equ oe inner join oa_sale_order o\r\n  				on(oe.orderId=o.id)    where o.state='2'\r\n  union all \r\nselect if(s.orderCode='',s.orderTempCode,s.orderCode) as orderCode,s.timeLimit,\r\n  			se.`productId` ,se.`number`,se.`executedNum`,\r\n            (se.`number`-se.`executedNum`) as notExeNum \r\n  			FROM oa_service_equ se inner join oa_sale_service s\r\n  				on(se.orderId=s.id)   where s.state='2'\r\n  union all \r\n  			 select if(l.orderCode='',l.orderTempCode,l.orderCode) as orderCode,l.beginTime,\r\n  			le.`productId` ,le.`number`,le.`executedNum`,\r\n            (le.`number`-le.`executedNum`) as notExeNum \r\n  			FROM oa_lease_equ le inner join oa_sale_lease l\r\n  				on(le.orderId=l.id)    where l.state='2'\r\n  union all \r\n  			 select if(r.orderCode='',r.orderTempCode,r.orderCode) as orderCode,r.timeLimit,\r\n  			re.`productId`,re.`number`,re.`executedNum`,\r\n            (re.`number`-re.`executedNum`) as notExeNum \r\n  			FROM oa_rdproject_equ re inner join `oa_sale_rdproject` r\r\n  				on(re.orderId=r.id)   where r.state='2'\r\n)psub left join \r\n			 (	\r\n             	select SUM( actNum ) as actNum,productId,productCode,productName from `oa_stock_inventory_info` i \r\n					where i.stockId<>-1  group by productId\r\n                )ig on(ig.productId=psub.productId)  order BY ig.productCode,psub.orderCode'
